							"\r",
							"// Danh sách cố định\r",
							"const brands = ['Mobifone', 'Vinaphone', 'Viettel', 'Vietnamobile'];\r",
							"const products = ['Nạp thẻ điện thoại', 'Voucher GotIt', 'Thẻ cào điện thoại', 'Mã giảm giá Momo'];\r",
							"const prices = ['100000', '200000', '50000', '100000'];\r",
							"\r",
							"const now = new Date();\r",
							"const yyyyMMdd = now.toISOString().slice(0, 10).replace(/-/g, '');\r",
							"\r",
							"const partnerCode = 'PARTNER_' + randomString(6);\r",
							"const voucherSerial = 'VC_' + yyyyMMdd + '_' + randomString(4);\r",
							"const productName = products[Math.floor(Math.random() * products.length)];\r",
							"const brand = brands[Math.floor(Math.random() * brands.length)];\r",
							"const price = prices[Math.floor(Math.random() * prices.length)];\r",
							"const link = 'https://c.gotit.vn/' + randomString(8);\r",
							"\r",
							"// Gán biến môi trường\r",
							"pm.environment.set(\"partnerCode\", partnerCode);\r",
							"pm.environment.set(\"voucherSerial\", voucherSerial);\r",
							"pm.environment.set(\"productName\", productName);\r",
							"pm.environment.set(\"brand\", brand);\r",
							"pm.environment.set(\"price\", price);\r",
							"pm.environment.set(\"link\", link);\r",
